<!DOCTYPE html>
<html>

{{> head }}

<body>
    <div class="navi-wrapper">
        <div class="navi-wrapper-content">
            {{> logo }}
            <div class="primary-list">
                <h3>Listeners</h3>
                <ul>
                    {{#each module.listeners}}
                        <li>
                            <a href="{{name}}.html">{{name}}</a>
                        </li>
                    {{/each}}
                </ul>
            </div>
            <div class="ui divider"></div>
            {{> currentModule }}
            {{> allModules }}
        </div>
    </div>

    <div class="ui vertical sidebar menu">
        <div class="navi-wrapper-sidebar">
            <div class="navi-wrapper-content">
                {{> logo }}
                <div class="primary-list">
                    <h3>Listeners</h3>
                    <ul>
                        {{#each module.listeners}}
                            <li>
                                <a href="{{name}}.html">{{name}}</a>
                            </li>
                        {{/each}}
                    </ul>
                </div>
                <div class="ui divider"></div>
                {{> currentModule }}
                {{> allModules }}
            </div>
        </div>
    </div>
    
    <div class="content-wrapper pusher">
        {{> mobMenu }}
        <div class="main-wrapper">
            <h1>Listeners : <span class="listeners">{{ listener.name }}</span> </h1>
            <p>{{{ listener.description }}}</p>

            <div class="constants">
                {{#if listener.initMethod}}
                    {{#listener.initMethod}}
                        {{> initMethod }}
                    {{/listener.initMethod}}
                {{/if}}

                {{#if listener.otherMethods}}
                    <h2>Methods</h2>
                    <div class="method-list">
                        {{#each listener.otherMethods}}
                            {{> methodSignature }}
                        {{/each}}
                    </div>
                {{/if}}

                {{#if listener.lifeCycleMethods}}
                    <h2>LifeCycle Methods</h2>
                    <div class="method-list">
                        {{#each listener.lifeCycleMethods}}
                            {{> methodSignature }}
                        {{/each}}
                    </div>
                {{/if}}

                {{#if listener.fields}}
                    {{#listener}}
                        <h2>Fields</h2>
                            {{> fields }}
                    {{/listener}}
                {{/if}}

                {{#if listener.otherMethods }}
                    <h2>Methods</h2>
                    {{#each listener.otherMethods }}
                        {{> method }}
                    {{/each}}
                {{/if}}

                {{#if listener.lifeCycleMethods }}
                    <h2>LifeCycle Methods</h2>
                    {{#each listener.lifeCycleMethods }}
                        {{> method }}
                    {{/each}}
                {{/if}}
            </div>
        </div>
    </div>
</body>

{{> footer }}
