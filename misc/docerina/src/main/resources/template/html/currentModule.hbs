    <a href="{{ rootPath }}{{ module.id }}/index.html">
        <h3>{{module.id}}</h3>
    </a>
    <ul>
        {{#if module.records}}
        <li>
            <a href="{{ ../rootPath }}{{ ../module.id }}/index.html#records">Records</a>
        </li>
        {{/if}}
        {{#if module.objects}}
            <li>
                <a href="{{ ../rootPath }}{{ ../module.id }}/index.html#objects">Objects</a>
