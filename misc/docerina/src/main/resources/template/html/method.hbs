<div class="method-content">
    <div class="main-method-title">
        <h2 id="{{name}}">{{name}}</h2>
        <p class="method-types">({{{ paramSummary parameters }}})</p>
    </div>
    <p>{{{ description }}}</p>
     {{#if parameters}}
        <div class="data-wrapper">
            <h3 class="param-title">Parameters</h3>
            {{#each parameters}}    
            <div class="params-listing">
                <ul>
                    <li> {{ name }}<span class="type">  {{{ typeName type }}} </span>{{#if defaultValue}}- {{ defaultValue }}{{/if}}</li>
                    <li>
                        <p>{{{ description }}}</p>
                    </li>
                </ul>
            </div>
            {{/each}}
        </div>
    {{/if}}

        {{#if returnParameters}}
            {{#each returnParameters}}
            <div class="returns-listing">
                <ul>
                    <li> <h3 class="type">Return Type</h3> (<span class="type">{{{ typeName type }}}</span>)</li>
                    <li>{{{ description }}}</li>
                </ul>
            </div>
            {{/each}}
        {{/if}}
</div>