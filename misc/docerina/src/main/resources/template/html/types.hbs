<!DOCTYPE html>
<html>

{{> head }}

<body>
<div class="ui grid">
    <div class="navi-wrapper two wide column">
        <div class="navi-wrapper-content">
            {{> logo }}
            <div class="primary-list">
                <h3>Types</h3>
                <ul>
                    {{#each module.unionTypes}}
                        <li>
                            <a href="#{{name}}">{{name}}</a>
                        </li>
                    {{/each}}
                </ul>
            </div>
            <div class="ui divider"></div>
            {{> currentModule }}
            {{> allModules }}
        </div>
    </div>

    <div class="fourteen wide column content-wrapper">
        <div class="main-wrapper">
            <h1>Types</h1>
            <div class="types">
                <div class="data-wrapper">
                    {{#each types}}
                        <div class="params-listing">
                            <ul>
                                <li id="{{ name }}"><b>{{ name }}</b><span class="type">  {{{ unionTypeSummary
                                        memberTypes }}}</span></li>
                                <li>
                                    {{{ description }}}
                                </li>
                            </ul>
                        </div>
                    {{/each}}
                    {{#each module.finiteTypes }}
                        <div class="params-listing">
                            <ul>
                                <li id="{{ name }}"><b>{{ name }}</b><span class="type">  {{{ pipeJoin valueSpace }}}</span></li>
                                <li>
                                    {{{ description }}}
                                </li>
                            </ul>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>
</body>
