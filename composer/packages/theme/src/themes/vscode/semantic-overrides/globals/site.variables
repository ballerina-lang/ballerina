/*-------------------
       Fonts
--------------------*/

@fontName          : -apple-system, BlinkMacSystemFont, Segoe WPC, Segoe UI, HelveticaNeue-Light, Ubuntu, Droid Sans, sans-serif;
@googleFontName    : 'Lato';

/*-------------------
      Base Sizes
--------------------*/

@emSize   : 12px;
@fontSize : 12px;


/*-------------------
    Border Radius
--------------------*/

@relativeBorderRadius: 0;
@absoluteBorderRadius: 0;

@defaultBorderRadius: @absoluteBorderRadius;